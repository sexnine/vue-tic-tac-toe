{
  "name": "vite-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "serve": "vite --host",
    "build": "npm run lint && vue-tsc --noEmit && vite build",
    "build-nolint": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint --fix \"**/*.{vue,js,ts}\" || exit 0",
    "prettier": "prettier --write \"**/*.{vue,js,ts,json}\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons-5": "npm:@fortawesome/free-solid-svg-icons@^5.15.4",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@vueuse/core": "^8.6.0",
    "firebase": "^9.8.3",
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@babel/types": "^7.18.4",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.17.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.1.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vue-tsc": "^0.37.5"
  }
}
